#!/bin/bash

# Get the current working directory and its parent directory
cwd=$(pwd)
parent=$(dirname "$cwd")

# Loop through all HTML files in the current directory
for file in *.html
do
  # Check if the file already exists in the parent directory and if it's older than the current version
  if [[ -f "$parent/$file" && "$cwd/$file" -nt "$parent/$file" ]]
  then
    # Copy the file to the parent directory
    cp "$cwd/$file" "$parent/$file"
  fi
done

